{
  "name": "docclone",
  "productName": "docclone",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start | nodemon backend/server.js --exec babel-node --presets es2015,stage-2",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src",
    "backend-dev": "nodemon backend/server.js --exec babel-node --presets es2015,stage-2",
    "backend-build": "babel backend/server -d server-dist --presets es2015,stage-2",
    "backend-serve": "node server-dist/index.js"
  },
  "keywords": [],
  "author": "dhruv",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "ignore": [
          "server",
          "server-dist"
        ]
      },
      "electronWinstallerConfig": {
        "name": "docclone"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "docclone"
      }
    }
  },
  "dependencies": {
    "@material-ui/core": "^1.4.0",
    "@material-ui/icons": "^1.1.0",
    "body-parser": "^1.18.3",
    "connect-mongo": "^2.0.1",
    "draft-js": "^0.10.5",
    "draft-js-color-picker": "^1.0.2",
    "electron-compile": "^6.4.3",
    "electron-devtools-installer": "^2.2.4",
    "electron-squirrel-startup": "^1.0.0",
    "es6-promise": "^4.2.4",
    "material-ui": "^0.20.1",
    "react-dropdown": "^1.5.0",
    "isomorphic-fetch": "^2.2.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "isomorphic-fetch": "^2.2.1",
    "mongoose": "^5.2.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3",
    "react-particles-js": "^2.2.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "semantic-ui-react": "^0.82.0",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "electron-compilers": "^5.9.0",
    "electron-forge": "^5.2.2",
    "electron-prebuilt-compile": "2.0.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.10.0",
    "nodemon": "^1.18.2"
  },
  "proxy": "http://localhost:3000/"
}
